
ScreenManager:
    Screen:
        id:pagina_principal
        name:"Process simulation"
        FloatLayout:
            id:Principalfloat

            Button:
                id:projected_demand
                text:"Start"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.15,"y":0.10}
                background_color:(0.5,0.1,1,1)
                on_release:app.get_demand()

            Button:
                id:real_demand
                text:"Requests"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.75,"y":0.25}
                background_color:(0.1,1,0.5,1)
                on_release:app.real_demand()

            Button:
                id:Raw
                text:"RMWH"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.15,"y":0.85}
                on_release:app.RMWH()

            Button:
                id:add_rm
                text:"Add \nRaw Material"
                size_hint:(0.15,0.1)
                pos_hint:{"x":0.15,"y":0.55}
                on_release:app.menu.open()


            Button:
                id:S1
                text:"Stage 1"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.40,"y":0.85}
                on_release:app.S1()

            Button:
                id:S2
                text:"Stage 2"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.40,"y":0.65}
                on_release:app.S2()

            Button:
                id:S3
                text:"Stage 3"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.40,"y":0.45}
                on_release:app.S3()

            Button:
                text:"Process \nnoise"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.40,"y":0.25}
                on_release:app.Noice()

            Button:
                text:"Summary"
                size_hint:(0.1,0.1)
                pos_hint:{"x":0.75,"y":0.75}
                on_release:app.Summary()